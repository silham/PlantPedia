// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Plants {
  id           String  @id @default(uuid())
  common_name  String
  other_name_1 String?
  other_name_2 String?
  other_name_3 String?
}
